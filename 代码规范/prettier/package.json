{
  "name": "prettier",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --debug src/*",
    "lint:write": "eslint --debug src/* --fix",
    "prettier": "prettier --write src/*"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:write && npm run prettier && git add ."
    }
  },
  "lint-staged": {
    "*.(js|jsx|vue)": ["npm run lint:write", "npm run prettier", "git add"]
  },
  "devDependencies": {
    "eslint": "^6.2.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.5",
    "prettier": "1.18.2"
  }
}